{
  "name": "ethereum-lite-explorer",
  "version": "1.0.0-beta.1",
  "private": false,
  "description": "Ethereum Lite Explorer",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run build-js-prod && npm run check-prod-bundle-size",
    "build-dev": "npm run clean && npm run build-js-dev",
    "check-prod-bundle-size": "bundlesize -f \"dist/js/app.bundle.js\" -s 850kB -c none",
    "clean": "rimraf ./dist",
    "build-js-prod": "webpack --config=./webpack.config.prod.js",
    "build-js-dev": "webpack --config=./webpack.config.dev.js",
    "watch": "webpack-runner --config=./webpack.config.dev.js --watch",
    "install": "cd dev && npm install && cd .. || true",
    "start": "cd dev && npm start && cd ..",
    "test": "mocha",
    "test-coverage": "nyc npm test",
    "deploy-gh": "npm run build  && cp 404.html ./dist/404.html && cp config.deploy.json ./dist/config.json && echo 'lite-explorer.aleth.io' > ./dist/CNAME && gh-pages -d dist -m 'gh deploy [ci skip]'"
  },
  "keywords": [
    "ethereum",
    "block explorer",
    "ethstats",
    "alethio",
    "lite explorer"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/fontfaceobserver": "0.0.6",
    "@types/gapi": "0.0.36",
    "@types/gapi.auth2": "0.0.50",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.12.2",
    "@types/qrcode": "^1.3.2",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-is": "^16.7.1",
    "@types/react-joyride": "^2.0.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-router-hash-link": "^1.2.1",
    "@types/react-transition-group": "^2.0.16",
    "@types/recharts": "^1.1.16",
    "@types/vis": "^4.21.12",
    "@types/web3": "^1.0.18",
    "@types/webpack-env": "^1.13.9",
    "bundlesize": "github:siddharthkp/bundlesize#brotli-optional",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "esm": "^3.2.22",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.0.1",
    "gh-pages": "^2.0.1",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "interpolate-html-plugin": "^3.0.0",
    "mocha": "^6.0.2",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.11",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tslib": "^1.9.3",
    "tslint": "^5.14.0",
    "typemoq": "^2.1.0",
    "typescript": "^3.4.1",
    "typescript-plugin-styled-components": "^1.4.2",
    "typescript-styled-plugin": "^0.14.0",
    "typescript-tslint-plugin": "^0.3.1",
    "url-loader": "^1.1.2",
    "webapp-webpack-plugin": "^2.6.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-runner": "^3.1.0"
  },
  "dependencies": {
    "@puzzl/browser": "^1.0.0-beta.2",
    "@puzzl/core": "^1.0.0-beta.1",
    "@sentry/browser": "^4.4.0",
    "bignumber.js": "^8.1.1",
    "datetime-difference": "^1.0.1",
    "ethstats-ui": "^1.0.0-beta.3",
    "fontfaceobserver": "^2.1.0",
    "identicon.js": "^2.3.3",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.3",
    "monaco-editor": "^0.16.2",
    "qrcode": "^1.3.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-graph-vis": "^1.0.2",
    "react-minimal-pie-chart": "^3.5.0",
    "react-popper": "^1.3.3",
    "react-router-dom": "^5.0.0",
    "react-router-hash-link": "^1.2.1",
    "react-transition-group": "^2.7.1",
    "styled-components": "^3.4.2"
  }
}
